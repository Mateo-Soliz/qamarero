	},
	"devDependencies": {
		"drizzle-kit": "^0.31.8",
		"@types/pg": "^8.15.6",
		"typescript": "catalog:",
		"@qamarero/config": "workspace:*",
		"tsx": "^4.21.0"
	},
	"dependencies": {
		"drizzle-orm": "^0.45.1",
		"pg": "^8.16.3",
		"dotenv": "catalog:",
		"zod": "catalog:",
		"tsx": "^4.21.0"
